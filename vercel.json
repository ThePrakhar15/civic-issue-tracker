{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "env": {
    "CI": "false"
  },
  "buildCommand": "npm run build --prefix frontend",
  "outputDirectory": "frontend/build",
  "routes": [
    { "src": "/(.*)", "dest": "frontend/build/index.html" }
  ]
}
